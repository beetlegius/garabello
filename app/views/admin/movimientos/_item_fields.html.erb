<tr class="nested-fields">
  <td style="width: 100px;"><%= f.input :cantidad, label: false %></td>
  <td>
    <%= f.input :producto_id, collection: [f.object.producto], label: false, input_html: { data: { behavior: :autocomplete, "ajax--url": @autocomplete_url } } %>
  </td>
  <td style="vertical-align: top !important;"><%= link_to_remove_association fa_icon(:remove), f, class: 'btn btn-danger btn-sm' %></td>
</tr>
