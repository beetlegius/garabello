<tr class="field">
  <td style="width: 120px;"><%= f.input :cantidad, label: false, input_html: { min: 1, step: 1, placeholder: true } %></td>
  <td><%= f.input :producto_id, collection: [f.object.producto], label: false, input_html: { data: { behavior: :autocomplete, "ajax--url": @autocomplete_url } } %></td>
  <td><%= link_to_remove_association fa_icon(:remove), f, { wrapper_class: 'field' } %></td>
</tr>
